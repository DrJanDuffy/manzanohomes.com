{
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check-all": "biome check .",
    "check-all:fix": "biome check --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lighthouse": "lhci autorun",
    "analyze": "vite-bundle-analyzer",
    "analyze:bundle": "vite build --mode analyze",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@playwright/test": "^1.40.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/enhanced-img": "^0.8.1",
    "@sveltejs/kit": "^2.0.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/svelte": "^7.6.0",
    "@storybook/svelte-vite": "^7.6.0",
    "@storybook/test": "^7.6.0",
    "@types/cookie": "^0.5.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^5.9.2",
    "@iconify/tailwind": "^1.2.0",
    "@iconify/utils": "^3.0.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "cookie": "^0.6.0",
    "date-fns": "^3.0.0",
    "geolib": "^3.3.4",
    "sharp": "^0.33.0",
    "web-vitals": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": ["biome check --write", "biome format --write"]
  }
}
