# Manzano Homes Environment Variables
# Copy this file to .env.local for local development

# Site Configuration
VITE_SITE_URL=https://manzanohome.com
VITE_SITE_NAME=Manzano Homes
VITE_SITE_DESCRIPTION=Premier real estate services in the Manzano neighborhood of Las Vegas, NV

# Contact Information
VITE_CONTACT_EMAIL=info@manzanohome.com
VITE_CONTACT_PHONE=+1-702-555-0123
VITE_CONTACT_ADDRESS=3693 Manzano Peak Ave, Las Vegas, NV 89121

# API Keys
VITE_REALSCOUT_API_KEY=your_realscout_api_key_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GA_TRACKING_ID=your_google_analytics_tracking_id_here

# RealScout Configuration
VITE_REALSCOUT_ENVIRONMENT=production
VITE_REALSCOUT_DEBUG=false

# Google Maps Configuration
VITE_GOOGLE_MAPS_DEFAULT_CENTER_LAT=36.1699
VITE_GOOGLE_MAPS_DEFAULT_CENTER_LNG=-115.1398
VITE_GOOGLE_MAPS_DEFAULT_ZOOM=13

# Manzano Neighborhood Specific
VITE_MANZANO_ZIP_CODE=89121
VITE_MANZANO_NEIGHBORHOOD=Manzano
VITE_MANZANO_CITY=Las Vegas
VITE_MANZANO_STATE=NV

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REALSCOUT=true
VITE_ENABLE_GOOGLE_MAPS=true
VITE_ENABLE_PWA=true

# Development
NODE_ENV=development
